  //     title: "Welcome Bonus",
  //     message: "Enjoy 20% off your next purchase with code WELCOME20.",
  //     timestamp: new Date(Date.now() - 5 * 24 * 60 * 60 * 1000), // 5 days ago
  //     isRead: true,
  //   },
  // ];

  // State management
  let notifications = [...mockNotifications];
  let currentTypeFilter = "all";
  let currentStatusFilter = "all";

  // Utility functions
  function getTimeAgo(date) {
    const now = new Date();
    const diffInSeconds = Math.floor((now.getTime() - date.getTime()) / 1000);

    if (diffInSeconds < 60) return "Just now";
    if (diffInSeconds < 3600) return `${Math.floor(diffInSeconds / 60)}m ago`;
    if (diffInSeconds < 86400)
      return `${Math.floor(diffInSeconds / 3600)}h ago`;
    if (diffInSeconds < 604800)
      return `${Math.floor(diffInSeconds / 86400)}d ago`;
    return date.toLocaleDateString();
  }

  function getNotificationIcon(type) {
    const icons = {
      order: "package",
      promotion: "gift",
      account: "credit-card",
      system: "bell",
    };
    return icons[type] || "bell";
  }

  function getCounts() {
    const all = notifications.length;
    const unread = notifications.filter((n) => !n.isRead).length;
    const order = notifications.filter((n) => n.type === "order").length;
    const promotion = notifications.filter(
      (n) => n.type === "promotion"
    ).length;
    const account = notifications.filter((n) => n.type === "account").length;
    const system = notifications.filter((n) => n.type === "system").length;

    return { all, unread, order, promotion, account, system };
  }

  function getFilteredNotifications() {
    return notifications.filter((notification) => {
      const typeMatch =
        currentTypeFilter === "all" || notification.type === currentTypeFilter;
      const statusMatch =
        currentStatusFilter === "all" ||
        (currentStatusFilter === "read" && notification.isRead) ||
        (currentStatusFilter === "unread" && !notification.isRead);

      return typeMatch && statusMatch;
    });
  }

  // Render functions
  function renderNotificationCard(notification) {
    const timeAgo = getTimeAgo(notification.timestamp);
    const icon = getNotificationIcon(notification.type);

    return `
        <div class="notification-card ${
          notification.isRead ? "read" : "unread"
        }" data-id="${notification.id}">
            ${
              !notification.isRead ? '<div class="unread-indicator"></div>' : ""
            }
            <div class="notification-content">
                <div class="notification-main">
                    <div class="notification-body">
                        <div class="notification-icon ${notification.type}">
                            <i data-lucide="${icon}"></i>
                        </div>
                        <div class="notification-text">
                            <h3 class="notification-title">${
                              notification.title
                            }</h3>
                            <p class="notification-message">${
                              notification.message
                            }</p>
                            ${
                              notification.metadata
                                ? `
                                <div class="notification-metadata">
                                    ${
                                      notification.metadata.orderId
                                        ? `
                                        <span>
                                            <i data-lucide="package"></i>
                                            Order #${notification.metadata.orderId}
                                        </span>
                                    `
                                        : ""
                                    }
                                    ${
                                      notification.metadata.amount
                                        ? `
                                        <span>$${notification.metadata.amount.toFixed(
                                          2
                                        )}</span>
                                    `
                                        : ""
                                    }
                                </div>
                            `
                                : ""
                            }
                        </div>
                    </div>
                    <div class="notification-actions">
                        <button class="action-btn read-btn ${
                          notification.isRead ? "read" : ""
                        }" 
                                onclick="toggleRead('${notification.id}')"
                                title="${
                                  notification.isRead
                                    ? "Mark as unread"
                                    : "Mark as read"
                                }">
                            <i data-lucide="check-circle"></i>
                        </button>
                        <button class="action-btn delete-btn" 
                                onclick="deleteNotification('${
                                  notification.id
                                }')"
                                title="Delete notification">
                            <i data-lucide="x"></i>
                        </button>
                    </div>
                </div>
                <div class="notification-footer">
                    <div class="notification-time">
                        <i data-lucide="clock"></i>
                        <span>${timeAgo}</span>
                    </div>
                    <a href="#" class="view-details">View Details</a>
                </div>
            </div>
        </div>
    `;
  }

  function renderNotifications() {
    const container = document.getElementById("notifications-container");
    const emptyState = document.getElementById("empty-state");
    const filteredNotifications = getFilteredNotifications();

    if (filteredNotifications.length === 0) {
      container.style.display = "none";
      emptyState.style.display = "block";

      const hasFilters =
        currentTypeFilter !== "all" || currentStatusFilter !== "all";
      emptyState.innerHTML = `
            <div class="empty-icon">
                <i data-lucide="${hasFilters ? "bell" : "check-circle"}"></i>
            </div>
            <h3>${hasFilters ? "No notifications found" : "All caught up!"}</h3>
            <p>${
              hasFilters
                ? "Try adjusting your filters to see more notifications."
                : "You have no new notifications. Check back later for updates on your orders and account."
            }</p>
        `;
    } else {
      container.style.display = "flex";
      emptyState.style.display = "none";
      container.innerHTML = filteredNotifications
        .map(renderNotificationCard)
        .join("");
    }

    // Re-initialize Lucide icons
    lucide.createIcons();
  }

  function updateCounts() {
    const counts = getCounts();

    // Update header
    document.getElementById("notification-count").innerHTML = `${
      counts.all
    } total notifications ${
      counts.unread > 0
        ? `<span class="unread-badge">${counts.unread} unread</span>`
        : ""
    }`;

    // Update filter counts
    document.getElementById("count-all").textContent = counts.all;
    document.getElementById("count-order").textContent = counts.order;
    document.getElementById("count-promotion").textContent = counts.promotion;
    document.getElementById("count-account").textContent = counts.account;
    document.getElementById("count-system").textContent = counts.system;

    // Update status select
    const statusSelect = document.getElementById("status-select");
    statusSelect.innerHTML = `
        <option value="all">All</option>
        <option value="unread">Unread (${counts.unread})</option>
        <option value="read">Read</option>
    `;
    statusSelect.value = currentStatusFilter;

    // Show/hide header buttons
    const markAllBtn = document.getElementById("mark-all-read");
    const clearAllBtn = document.getElementById("clear-all");

    markAllBtn.style.display = counts.unread > 0 ? "inline-flex" : "none";
    clearAllBtn.style.display = counts.all > 0 ? "inline-flex" : "none";
  }

  // Event handlers
  function toggleRead(id) {
    const notification = notifications.find((n) => n.id === id);
    if (notification) {
      notification.isRead = !notification.isRead;
      updateUI();
    }
  }

  function deleteNotification(id) {
    notifications = notifications.filter((n) => n.id !== id);
    updateUI();
  }

  function markAllAsRead() {
    notifications.forEach((n) => (n.isRead = true));
    updateUI();
  }

  function clearAll() {
    notifications = [];
    updateUI();
  }

  function setTypeFilter(type) {
    currentTypeFilter = type;

    // Update active filter button
    document.querySelectorAll(".filter-btn").forEach((btn) => {
      btn.classList.remove("active");
    });
    document.querySelector(`[data-type="${type}"]`).classList.add("active");

    updateUI();
  }

  function setStatusFilter(status) {
    currentStatusFilter = status;
    updateUI();
  }

  function updateUI() {
    updateCounts();
    renderNotifications();
  }

  // Initialize the app
  function init() {
    // Set up event listeners
    document
      .getElementById("mark-all-read")
      .addEventListener("click", markAllAsRead);
    document.getElementById("clear-all").addEventListener("click", clearAll);

    // Type filter buttons
    document.querySelectorAll(".filter-btn").forEach((btn) => {
      btn.addEventListener("click", () => {
        setTypeFilter(btn.dataset.type);
      });
    });

    // Status filter select
    document.getElementById("status-select").addEventListener("change", (e) => {
      setStatusFilter(e.target.value);
    });

    // Initial render
    updateUI();

    // Initialize Lucide icons
    lucide.createIcons();
  }

  // Start the app when DOM is loaded
  document.addEventListener("DOMContentLoaded", init);
</script>
